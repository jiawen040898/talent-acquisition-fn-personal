{
    "name": "talent-acquisition-fn",
    "version": "1.18.0",
    "author": "Pulsifi <dev-team@pulsifi.me>",
    "license": "UNLICENSED",
    "description": "Talent acquisition lambda",
    "private": true,
    "engines": {
        "node": ">=20.0.0",
        "yarn": ">=1.22.19"
    },
    "scripts": {
        "prepare": "is-ci || husky install",
        "build": "yarn install --frozen-lockfile --prefer-offline && yarn install --frozen-lockfile --prefer-offline --cwd layer/nodejs",
        "format": "pretty-quick --staged && prettier --loglevel \"warn\" --debug-check \"src/**/*.ts\" \"tests/**/*.ts\"",
        "lint": "yarn format && eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
        "test": "jest --watch",
        "test:ci": "jest --ci --coverage",
        "cdk": "cdk",
        "biome:format": "biome format --write ./cdk",
        "biome:lint": "biome lint ./cdk",
        "biome:check": "biome check --apply ./cdk"
    },
    "devDependencies": {
        "@actions/core": "1.10.1",
        "constructs": "10.3.0",
        "aws-cdk": "2.141.0",
        "aws-cdk-lib": "2.141.0",
        "@pulsifi/custom-aws-cdk-lib": "1.14.1",
        "@biomejs/biome": "1.7.3",
        "@mrgrain/cdk-esbuild": "5.1.0",
        "esbuild-node-externals": "1.13.1",
        "esbuild-plugin-copy": "2.1.1",
        "esbuild-plugin-time": "1.0.0",
        "esbuild-plugin-typescript-decorators": "0.1.0",
        "@pulsifi/fn": "3.0.6",
        "amazon-s3-uri": "0.1.1",
        "axios": "1.6.7",
        "pg": "8.11.3",
        "typeorm": "0.3.17",
        "unleash-client": "4.1.1",
        "@aws-sdk/client-s3": "^3.523.0",
        "@aws-sdk/client-sns": "^3.523.0",
        "@aws-sdk/client-sqs": "^3.523.0",
        "@aws-sdk/middleware-retry": "3.374.0",
        "@aws-sdk/util-stream-node": "^3.374.0",
        "@commitlint/cli": "17.8.0",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/npm": "11.0.0",
        "@types/aws-lambda": "8.10.133",
        "@types/cache-manager": "4.0.3",
        "@types/cache-manager-redis-store": "2.0.2",
        "@types/jest": "29.5.12",
        "@types/node": "20.11.21",
        "@types/pg": "8.10.5",
        "@types/serverless": "3.12.22",
        "@types/uuid": "9.0.8",
        "@typescript-eslint/eslint-plugin": "6.8.0",
        "@typescript-eslint/parser": "6.8.0",
        "aws-sdk-client-mock": "^2.1.1",
        "better-sqlite3": "8.5.0",
        "commitlint-plugin-function-rules": "2.0.2",
        "eslint": "8.51.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "factory.ts": "1.4.1",
        "husky": "8.0.3",
        "is-ci": "3.0.1",
        "jest": "29.7.0",
        "jest-junit": "16.0.0",
        "lint-staged": "15.0.1",
        "mockdate": "3.0.5",
